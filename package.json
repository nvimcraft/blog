{
  "name": "rjleyva-blog",
  "author": "RJ Leyva <rjleyva@tutanota.de>",
  "description": "Personal blog documenting web development insights and ideas.",
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "private": true,
  "scripts": {
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch"
  },
  "packageManager": "pnpm@10.28.2",
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@7.2.5"
    }
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.1",
    "eslint": "^10.0.1",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0"
  }
}
